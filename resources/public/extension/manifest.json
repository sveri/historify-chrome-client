{
  "manifest_version": 2,

  "name": "Historify Client",
  "description": "Connects to historify to save browser history",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Historify Client"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://localhost:8080/",
    "https://*.sveri.de/"
  ]
}
